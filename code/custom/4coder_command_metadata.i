    if (string_match(string_prefix(line, 3), SCu8((u8*)("-->"), (u64)(sizeof("-->") - 1)))){
        String_Const_u8 sub = string_skip(line, 3);
        sub = string_skip_chop_whitespace(sub);
        pos = (u64)(sub.str - line.str);
    }
